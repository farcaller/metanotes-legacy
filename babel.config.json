{
  "presets": [
    "module:metro-react-native-babel-preset",
    [
      "@babel/env",
      {
        "targets": {
          "node": true
        },
        "useBuiltIns": "usage",
        "corejs": "3.8.3"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-flow-strip-types",
    "babel-plugin-transform-typescript-metadata",
    ["@babel/plugin-proposal-decorators", { "legacy": true }]
  ]
}
